{
	"name": "df_Metadata_bronzeToSilver_Sales",
	"properties": {
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "ds_bronze_Sales",
						"type": "DatasetReference"
					},
					"name": "SalesDataIn"
				}
			],
			"sinks": [
				{
					"dataset": {
						"referenceName": "ds_AzureSQLDB_SalesTBL",
						"type": "DatasetReference"
					},
					"name": "SalesDataLoadtoAzureSQL"
				}
			],
			"transformations": [
				{
					"name": "aggregate1"
				},
				{
					"name": "select1"
				}
			],
			"scriptLines": [
				"parameters{",
				"     p_Sales_FileName as string",
				"}",
				"source(output(",
				"          SaleID as string,",
				"          CustomerID as string,",
				"          ProductID as string,",
				"          Quantity as string,",
				"          SaleDate as string,",
				"          Revenue as string",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     ignoreNoFilesFound: false) ~> SalesDataIn",
				"SalesDataIn aggregate(groupBy(SaleID,",
				"          CustomerID,",
				"          ProductID,",
				"          Quantity,",
				"          SaleDate,",
				"          Revenue),",
				"     count = count()) ~> aggregate1",
				"aggregate1 select(mapColumn(",
				"          SaleID,",
				"          CustomerID,",
				"          ProductID,",
				"          Quantity,",
				"          SaleDate,",
				"          Revenue",
				"     ),",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true) ~> select1",
				"select1 sink(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     input(",
				"          CustomerID as integer,",
				"          First_Name as string,",
				"          Last_Name as string,",
				"          Gender as string",
				"     ),",
				"     deletable:false,",
				"     insertable:false,",
				"     updateable:false,",
				"     upsertable:false,",
				"     format: 'table',",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true,",
				"     errorHandlingOption: 'stopOnFirstError',",
				"     mapColumn(",
				"          productid,",
				"          productname,",
				"          productcategory",
				"     )) ~> SalesDataLoadtoAzureSQL"
			]
		}
	}
}